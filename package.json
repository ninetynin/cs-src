{
    "name": "cs-src",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "build": "rm -rf build && vite build && touch build/.nojekyll",
        "deploy": "npm run build && npx gh-pages -d .svelte-kit/static/build -t true"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/adapter-static": "^1.0.0-next.47",
        "@sveltejs/kit": "next",
        "gh-pages": "^4.0.0",
        "svelte": "^3.44.0",
        "svelte-check": "^2.7.1",
        "svelte-preprocess": "^4.10.6",
        "tslib": "^2.3.1",
        "typescript": "^4.7.4",
        "vite": "^3.1.0"
    },
    "type": "module"
}